{
  "name": "ai-committer",
  "displayName": "AI Committer",
  "publisher": "your-publisher",
  "engines": {
    "vscode": ">=1.60.0"
  },
  "activationEvents": [
    "onCommand:aiCommit.generate"
  ],
  "contributes": {
    "commands": [
      {
        "command": "aiCommit.generate",
        "title": "AI: Generate Commit Message and Commit"
      }
    ],
    "configuration": {
      "title": "AI Committer",
      "properties": {
        "aiCommit.geminiApiKey": {
          "type": "string",
          "default": "",
          "description": "Your Gemini API Key for generating commit messages."
        }
      }
    }
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/vscode": "^1.60.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@google/generative-ai": "^0.1.3"
  }
}
